{
  "name": "gegrids",
  "version": "1.0.0",
  "description": "This is the first free, customizable creator of raster data from Google Earth.",
  "main": "index.html",
  "scripts": {
    "test": "ava \"app/**/*.test.js\" --verbose",
    "w:test": "npm run test -- --watch",
    "w:cover": "nodemon --quiet --watch app --exec npm run cover -s",
    "dev": "webpack-dev-server --progress --colors --config",
    "cover": "nyc npm run test",
    "ch:coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andybp85/GERasterCreator.git"
  },
  "author": "Andrew Stanish",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/andybp85/GERasterCreator/issues"
  },
  "homepage": "https://github.com/andybp85/GERasterCreator#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-register": "^6.9.0",
    "core-js": "^2.4.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "nyc": "^6.4.4",
    "sass-loader": "^3.2.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
