{
  "name": "gegrids",
  "version": "1.0.0",
  "description": "This is the first free, customizable creator of raster data from Google Earth.",
  "main": "index.html",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --progress --colors --config",
    "test": "NODE_ENV=test ava \"gegrids/**/*.test.js\" --verbose",
    "cover": "NODE_ENV=test nyc npm run test",
    "chcoverage": "NODE_ENV=test nyc check-coverage",
    "flow": "NODE_ENV=development flow",
    "wtest": "NODE_ENV=test npm run test -- --watch",
    "wcover": "NODE_ENV=test nodemon --quiet --watch gegrids --exec npm run cover -s",
    "wflow": "NODE_ENV=development nodemon --quiet --watch gegrids --exec npm run flow -s"
  },
  "ava": {
    "require": [
      "./test/helpers/setup-test-env.js"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "lines": 100,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "check-coverage": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andybp85/GERasterCreator.git"
  },
  "author": "Andrew Stanish",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/andybp85/GERasterCreator/issues"
  },
  "homepage": "https://github.com/andybp85/GERasterCreator#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-webpack-alias": "^1.3.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "core-js": "^2.4.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "nyc": "^6.4.4",
    "sass-loader": "^3.2.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
